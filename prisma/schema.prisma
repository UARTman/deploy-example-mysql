generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://remix:test@localhost:3306/remixtodos"
}

model Task {
  id Int @id @default(autoincrement())
  title String
  done Boolean
}